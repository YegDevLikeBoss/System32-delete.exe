{
  "manifest_version": 2,
 
  "name": "No style, just my taste",
  "description": "This extension runs Python code.",
  "version": "0.0.0.0312",
 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus"
  ],
    "background": { 
      "scripts": ["script.js"]
    }
}